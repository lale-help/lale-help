h1 = t('.title')

table.default-table
  thead
    tr
      th = Project.human_attribute_name("name")
      th = Project.human_attribute_name("working_group")
      th = Project.human_attribute_name("organizer")

  tbody
    - @projects.each do |project|
      tr
        td = link_to_project(project)
        td = link_to_working_group(project.working_group)
        td
          - project.active_admins.each do |user|
            li= link_to_user(user)